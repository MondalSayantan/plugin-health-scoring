{
    "name": "Java 17 with Node.js and PostgreSQL using sdkman",
    "dockerFile": "Dockerfile",
    "remoteUser": "vscode",
    "extensions": [
        "redhat.vscode-java",
        "ms-vscode.vscode-typescript-tslint-plugin",
        "ms-azuretools.vscode-docker",
        "ms-vscode.vscode-node-azure-pack"
    ],
    "settings": {
        "java.home": "/root/.sdkman/candidates/java/17.0.3.ms-zulu",
        "java.configuration.maven.userSettings": "/root/.m2/settings.xml",
        "maven.terminal.useJavaHome": true,
        "maven.executable.path": "/root/.sdkman/candidates/maven/current/bin/mvn",
        "maven.terminal.customEnv": {
            "JAVA_HOME": "/root/.sdkman/candidates/java/17.0.3.ms-zulu",
            "PATH": "/root/.sdkman/candidates/java/17.0.3.ms-zulu/bin:/root/.sdkman/candidates/maven/current/bin:$PATH"
        },
        "typescript.tsdk": "node_modules/typescript/lib"
    },
    "forwardPorts": [
        5432
    ]
}
